@model BecketLee.ViewModels.PartnerViewModel
@{
    ViewBag.Title = @Model.PartnerName;
}


<div class="row">

    <div class="col-8">

        <div class="card">
            <div class="card card-header"><h3>Biography</h3></div>
            <div class="card card-body ">

                <div id="BiographyHtml">@Html.Raw( Model.BiographyHtml )</div>
            </div>
        </div>
    </div>
    <div class="col-4">
        <div class="card">
            <div class="card card-header"><h3 class="text-center">@Model.PartnerName</h3></div>
            <div class="card card-body">

                <div class="text-center">
                    <img src="@Model.FileUrl" alt="@Model.PartnerName" class="partnerImage" />
                </div>
            </div>
        </div>
    </div>
</div>
